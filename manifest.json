{
    "manifest_version": 3,

    "name": "Remove Duplicated Codes",
    "description": "This extension removes duplicated codes on github.",
    "version": "1.0",
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-48.png",
        "64": "images/icon-128.png"
    },
    "content_scripts": [{
        "matches": ["https://github.com/*"],
        "js": ["myExtension.js"]
    }]
}